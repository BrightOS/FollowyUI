<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="text" format="string" />
    <attr name="font" format="reference" />

    <declare-styleable name="FollowyAppBar">
        <attr name="startIcon" format="reference" />
        <attr name="endIcon" format="reference" />
        <attr name="startIconTinting" format="enum">
            <enum name="enabled" value="0"/>
            <enum name="disabled" value="1"/>
        </attr>
        <attr name="endIconTinting" format="enum">
            <enum name="enabled" value="0"/>
            <enum name="disabled" value="1"/>
        </attr>
        <attr name="text" />
        <attr name="font" />
        <attr name="configureInsets" format="boolean" />
        <attr name="toolbarPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FollowyToggleButton">
        <attr name="icon" format="reference" />
        <attr name="iconTinting" format="enum">
            <enum name="enabled" value="0"/>
            <enum name="disabled" value="1"/>
        </attr>
        <attr name="text" />
        <attr name="font" />
    </declare-styleable>
</resources>